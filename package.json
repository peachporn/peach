{
  "name": "peach",
  "version": "3.0.0-alpha",
  "description": "porn organizer",
  "bin": "packages/app/dist/index.js",
  "repository": "https://github.com/peachporn/peach.git",
  "author": "GalateaPolymorph <galateapolymorph@gmail.com>",
  "license": "GPL-3.0",
  "private": true,
  "scripts": {
    "dev": "cd dist && nodemon app.js",
    "watch": "run-p watch:*",
    "watch:app": "webpack --watch --mode=development",
    "watch:frontend": "parcel watch src/frontend/index.html --public-url /assets --out-dir dist/frontend",
    "watch:migrations": "webpack --watch --mode=development --config webpack.migrations.config.js",
    "build": "run-p build:*",
    "build:app": "webpack --mode=production",
    "build:frontend": "parcel build src/frontend/index.html --public-url /assets --out-dir dist/frontend",
    "build:migrations": "webpack --mode=production --config webpack.migrations.config.js",
    "lint": "eslint src",
    "generate": "run-p generate:*",
    "generate:types": "graphql-codegen --config codegen.yml",
    "generate:prisma": "prisma generate",
    "migration:run": "prisma migrate up --experimental",
    "migration:save": "prisma migrate save --experimental",
    "studio": "prisma studio --experimental"
  },
  "alias": {
    "react": "preact/compat",
    "react-dom": "preact/compat"
  },
  "dependencies": {
    "@apollo/client": "^3.0.0-rc.2",
    "@apollo/react-hooks": "^3.1.5",
    "@graphql-tools/merge": "^6.0.5",
    "@graphql-tools/schema": "^6.0.5",
    "@prisma/cli": "^2.0.0",
    "@prisma/client": "^2.0.0",
    "@seznam/compose-react-refs": "^1.0.4",
    "apollo-boost": "^0.4.9",
    "apollo-server": "^2.14.1",
    "apollo-server-core": "^2.14.3",
    "apollo-server-express": "^2.14.1",
    "body-parser": "^1.19.0",
    "camelcase": "^6.0.0",
    "cron": "^1.8.2",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "glob": "^7.1.6",
    "graphql": "^14.5.3",
    "mkdirp": "^1.0.4",
    "pino": "^6.3.1",
    "pino-pretty": "^4.0.0",
    "preact": "^10.4.4",
    "preact-render-to-string": "^5.1.4",
    "ramda": "^0.27.0",
    "react-hook-form": "^5.7.2",
    "react-router-dom": "^5.2.0",
    "react-toastify": "^6.0.5",
    "which": "^2.0.2"
  },
  "devDependencies": {
    "@babel/core": "^7.10.1",
    "@graphql-codegen/cli": "^1.15.0",
    "@graphql-codegen/fragment-matcher": "^1.15.0",
    "@graphql-codegen/typescript": "^1.15.0",
    "@graphql-codegen/typescript-operations": "^1.15.0",
    "@graphql-codegen/typescript-resolvers": "^1.15.0",
    "@types/cron": "^1.7.2",
    "@types/eslint": "^6.8.1",
    "@types/glob": "^7.1.2",
    "@types/mkdirp": "^1.0.1",
    "@types/node": "^14.0.13",
    "@types/pino": "^6.0.1",
    "@types/pino-http": "^5.0.1",
    "@types/ramda": "^0.27.6",
    "@types/react-router-dom": "^5.1.5",
    "@types/which": "^1.3.2",
    "@typescript-eslint/eslint-plugin": "^2.24.0",
    "@typescript-eslint/parser": "^3.0.2",
    "copy-webpack-plugin": "^6.0.2",
    "eslint": "^7.1.0",
    "eslint-config-airbnb": "^18.1.0",
    "eslint-config-airbnb-typescript": "^7.2.1",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prefer-arrow": "^1.2.1",
    "eslint-plugin-prettier": "^3.1.3",
    "eslint-plugin-react": "^7.20.0",
    "nodemon": "^2.0.4",
    "npm-run-all": "^4.1.5",
    "parcel-bundler": "^1.12.4",
    "prettier": "^2.0.5",
    "stylus": "^0.54.7",
    "ts-loader": "^7.0.5",
    "typescript": "^3.9.3",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.11",
    "webpack-node-externals": "^1.7.2"
  }
}
